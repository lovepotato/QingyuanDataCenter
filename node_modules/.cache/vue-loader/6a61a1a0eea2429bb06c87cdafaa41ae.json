    <div class=\"org-name text-item\">{{ firstName }}</div>\n      <div class=\"divide-item\">\n      </div>\n      <div class=\"address text-item\">{{ lastName }}</div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport { mapGetters } from 'vuex'\nimport Breadcrumb from '@/components/Breadcrumb'\n\nexport default {\n  components: {\n    // Breadcrumb\n    // Hamburger\n  },\n  data() {\n    return {\n      firstName: '王洛东',\n      lastName: '街道办事处'\n    }\n  },\n  computed: {\n    ...mapGetters([\n      'sidebar',\n      'avatar'\n    ])\n  },\n  methods: {\n    toggleSideBar() {\n      this.$store.dispatch('app/toggleSideBar')\n    },\n    async logout() {\n      await this.$store.dispatch('user/logout')\n      this.$router.push(`/login?redirect=${this.$route.fullPath}`)\n    }\n  }\n}\n</script>\n\n<style lang=\"scss\" scoped>\n.navbar {\n  height: 50px;\n  overflow: hidden;\n  position: relative;\n  background: #D72324;\n  // box-shadow: 0 1px 4px rgba(0,21,41,.08);\n\n  .left-text {\n    height: 100%;\n    float: left;\n    width: 60%;\n    display: flex;\n    align-items: center;\n\n    .left-img {\n      margin-left: 26px;\n      margin-right: 10.6px\n    }\n\n    .left-title {\n      font-family: PingFangSC-Semibold;\n      font-size: 16px;\n      color: #FFFFFF;\n      letter-spacing: 0;\n      margin-right: 14px;\n    }\n\n    .left-number {\n      font-family: PingFangSC-Regular;\n      font-size: 12px;\n      color: #FFFFFF;\n      letter-spacing: 0;\n    }\n  }\n\n  .hamburger-container {\n    line-height: 46px;\n    height: 100%;\n    float: left;\n    cursor: pointer;\n    transition: background .3s;\n    -webkit-tap-highlight-color:transparent;\n\n    &:hover {\n      background: rgba(0, 0, 0, .025)\n    }\n  }\n\n  .breadcrumb-container {\n    float: left;\n  }\n\n  .right-menu {\n    display: flex;\n    height: 100%;\n    line-height: 50px;\n    width: 40%;\n    justify-content: flex-end;\n    &:focus {\n      outline: none;\n    }\n    .text-item{\n      width: 80px;\n      height: 50px;\n      line-height: 50px;\n      font-size: 12px;\n      text-align: center;\n      color: #fff;\n      &:last-child{\n        margin-right: 20px;\n      }\n    }\n    .divide-item{\n      margin: 16px 3px;\n      height: 18px;\n      width: 1px;\n      background-color: #fff;\n    }\n    .right-menu-item {\n      display: inline-block;\n      padding: 0 8px;\n      height: 100%;\n      font-size: 18px;\n      color: #5a5e66;\n      vertical-align: text-bottom;\n\n      &.hover-effect {\n        cursor: pointer;\n        transition: background .3s;\n\n        &:hover {\n          background: rgba(0, 0, 0, .025)\n        }\n      }\n    }\n\n    // .avatar-container {\n    //   margin-right: 30px;\n\n      .avatar-wrapper {\n        // width: 24px;\n        // height: 24px;\n        // margin-top: 5px;\n        width: 40px;\n        height: 50px;\n        line-height: 50px;\n        position: relative;\n        text-align: center;\n        .user-avatar {\n          cursor: pointer;\n          width: 40px;\n          height: 40px;\n          border-radius: 10px;\n          margin-top: 5px;\n        }\n\n        .el-icon-caret-bottom {\n          cursor: pointer;\n          position: absolute;\n          right: -20px;\n          top: 25px;\n          font-size: 12px;\n        }\n      }\n    // }\n  }\n}\n</style>\n"]}]}